{
    "name": "@arpadroid/i18n",
    "version": "1.0.3",
    "description": "Internationalization module",
    "exports": {
        ".": {
            "types": "./dist/@types/types.compiled.d.ts",
            "import": "./dist/arpadroid-i18n.js"
        }
    },
    "types": "./dist/@types/types.compiled.d.ts",
    "type": "module",
    "scripts": {
        "watch": "arpadroid-build --watch",
        "build": "arpadroid-build --minify",
        "dev": "arpadroid-build --watch --storybook",
        "test": "arpadroid-test --storybook --jest",
        "t": "npm run test -- --slim",
        "test:jest": "../module/node_modules/.bin/jest",
        "clean": "arpadroid-clean",
        "clean:build": "npm run clean && npm install",
        "check:types": "arpadroid-check-types",
        "initialize": "arpadroid-initialize"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arpadroid/i18n.git"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "l10n",
        "localization"
    ],
    "author": "Andres Vaquero @arpadroid <web.andresvaquero@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arpadroid/i18n/issues"
    },
    "homepage": "https://github.com/arpadroid/i18n#readme",
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@types/jest": "^30.0.0",
        "babel-jest": "^30.2.0",
        "glob": "^13.0.0"
    },
    "overrides": {
        "minimatch": "10.2.1",
        "ajv": "8.18.0"
    },
    "peerDependencies": {
        "@arpadroid/module": "file:../module",
        "@arpadroid/tools": "file:../tools"
    }
}
