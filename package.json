{
    "name": "@arpadroid/i18n",
    "version": "1.0.3",
    "description": "Internationalization module",
    "exports": "./dist/arpadroid-i18n.js",
    "types": "./dist/@types/types.compiled.d.ts",
    "type": "module",
    "scripts": {
        "build:dev": "arpadroid-build --watch",
        "build": "arpadroid-build --minify",
        "prepublish": "npm run build",
        "dev": "arpadroid-build --watch --storybook=6004 --watch",
        "test": "arpadroid-test --storybook --jest --port=6004",
        "test:ci": "npm run test -- --ci",
        "clean": "rm -rf ./dist ./node_modules package-lock.json",
        "clean:build": "npm run clean && npm install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arpadroid/i18n.git"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "l10n",
        "localization"
