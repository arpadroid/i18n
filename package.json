{
    "name": "@arpadroid/i18n",
    "version": "1.0.3",
    "description": "Internationalization module",
    "exports": "./dist/arpadroid-i18n.js",
    "module": "./dist/arpadroid-i18n.js",
    "type": "module",
    "scripts": {
        "generate-docs": "jsdoc2md src/services/*.js src/components/*/*.js > README.md",
        "build": "node ./node_modules/@arpadroid/arpadroid/scripts/build-project.mjs --project=i18n --minify",
        "build:prod": "npm run build -- --mode=production",
        "build:watch": "npm run build -- --watch",
        "build:watch:slim": "npm run build -- --watch --slim",
        "build:slim": "npm run build -- --slim",
        "prepublish": "npm run build",
        "storybook:build": "storybook build",
        "storybook:test": "test-storybook",
        "storybook": "npm run build -- --watch & npm run storybook:only",
        "storybook:only": "storybook dev -p 6011",
        "test": "npm run storybook:test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arpadroid/i18n.git"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "l10n",
        "localization"
    ],
    "author": "Andres Vaquero @arpadroid <web.andresvaquero@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arpadroid/i18n/issues"
    },
    "homepage": "https://github.com/arpadroid/i18n#readme",
    "devDependencies": {
        "@arpadroid/arpadroid": "file:../arpadroid",
        "@babel/core": "^7.23.9",
        "@babel/plugin-syntax-import-assertions": "^7.23.3",
        "@babel/preset-env": "^7.23.9",
        "@rollup/plugin-json": "^6.1.0",
        "@storybook/addon-a11y": "^8.1.1",
        "@storybook/addon-essentials": "^8.1.1",
        "@storybook/addon-interactions": "^8.1.1",
        "@storybook/addon-links": "^8.1.1",
        "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
        "@storybook/blocks": "^8.1.1",
        "@storybook/cli": "^8.1.1",
        "@storybook/test": "^8.1.1",
        "@storybook/test-runner": "^0.18.1",
        "@storybook/web-components": "^8.1.1",
        "@storybook/web-components-webpack5": "^8.1.1",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "^12.0.2",
        "html-webpack-plugin": "^5.6.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-junit": "^16.0.0",
        "jsdoc-to-markdown": "^8.0.1",
        "jsdom": "^24.0.0",
        "rollup": "^4.18.0",
        "rollup-plugin-copy": "^3.5.0",
        "storybook": "^8.1.1",
        "typescript": "^5.4.2",
        "yargs": "^17.7.2"
    },
    "peerDependencies": {
        "@arpadroid/tools": "file:../tools"
    }
}
