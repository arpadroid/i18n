{
    "name": "@arpadroid/i18n",
    "version": "1.0.3",
    "description": "Internationalization module",
    "exports": "./dist/arpadroid-i18n.js",
    "module": "./dist/arpadroid-i18n.js",
    "type": "module",
    "scripts": {
        "generate-docs": "jsdoc2md src/services/*.js src/components/*/*.js > README.md",
        "build": "node ./node_modules/@arpadroid/arpadroid/scripts/build-project.mjs --project=i18n --minify",
        "prepublish": "npm run build",
        "storybook": "npm run build -- --storybook=6004 --watch",
        "test": "node ./node_modules/@arpadroid/arpadroid/scripts/test-project.mjs --project=i18n --storybook --jest --port=6004"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arpadroid/i18n.git"
    },
    "keywords": [
        "i18n",
        "internationalization",
        "l10n",
        "localization"
    ],
    "author": "Andres Vaquero @arpadroid <web.andresvaquero@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arpadroid/i18n/issues"
    },
    "homepage": "https://github.com/arpadroid/i18n#readme",
    "devDependencies": {
        "@arpadroid/arpadroid": "file:../arpadroid",
        "@babel/core": "^7.23.9",
        "@babel/plugin-syntax-import-assertions": "^7.23.3",
        "@babel/preset-env": "^7.23.9",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "glob": "^11.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-junit": "^16.0.0",
        "jsdoc-to-markdown": "^8.0.1",
        "jsdom": "^24.0.0",
        "typescript": "^5.4.2",
        "yargs": "^17.7.2"
    },
    "peerDependencies": {
        "@arpadroid/tools": "file:../tools"
    }
}
