    },
    "homepage": "https://github.com/arpadroid/i18n#readme",
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@babel/plugin-syntax-import-assertions": "^7.23.3",
        "@babel/preset-env": "^7.23.9",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "^12.0.2",
        "html-webpack-plugin": "^5.6.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
